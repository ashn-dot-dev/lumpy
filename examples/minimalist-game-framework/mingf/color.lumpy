let color = {
    "type": "color",
    "new": function(r, g, b, a) {
        if a == null {
            a = 0xFF;
        }
        let self = {
            "r": math::clamp(r, 0x00, 0xFF),
            "g": math::clamp(g, 0x00, 0xFF),
            "b": math::clamp(b, 0x00, 0xFF),
            "a": math::clamp(a, 0x00, 0xFF),
        };
        setmeta(self.&, color);
        return self;
    },
};

return color;
