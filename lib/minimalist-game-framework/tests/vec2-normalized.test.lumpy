let vec2 = import("../vec2.lumpy");

dumpln(vec2::init(0, 0).normalized().fixed(3));
dumpln(vec2::init(+1, +1).normalized().fixed(3));
dumpln(vec2::init(-1, -1).normalized().fixed(3));
dumpln(vec2::init(math::cos(math::pi / 4), math::sin(math::pi / 4)).normalized().fixed(3));
