println("\", \".join([\"foo\", \"bar\", \"baz\"]) is " + repr(", ".join(["foo", "bar", "baz"])));
println("\", \".join([]) is " + repr(", ".join([])));
println("\"\".join([\"foo\", \"bar\", \"baz\"]) is " + repr("".join(["foo", "bar", "baz"])));
println("\"\".join([]) is " + repr("".join([])));
