println("(+123.45).round() is " + repr((+123.45).round()));
println("(-123.45).round() is " + repr((-123.45).round()));
println("(+123.54).round() is " + repr((+123.54).round()));
println("(-123.54).round() is " + repr((-123.54).round()));
try { Inf.round(); } else err { println("Inf.round() produced the error " + repr(err)); }
try { NaN.round(); } else err { println("NaN.round() produced the error " + repr(err)); }
################################################################################
