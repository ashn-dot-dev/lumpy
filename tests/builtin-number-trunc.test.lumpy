println("(+123.45).trunc() is " + repr((+123.45).trunc()));
println("(-123.45).trunc() is " + repr((-123.45).trunc()));
try { Inf.trunc(); } else err { println("Inf.trunc() produced the error " + repr(err)); }
try { NaN.trunc(); } else err { println("NaN.trunc() produced the error " + repr(err)); }
################################################################################
# (+123.45).trunc() is 123
# (-123.45).trunc() is -123
# Inf.trunc() produced the error "cannot convert float infinity to integer"
# NaN.trunc() produced the error "cannot convert float NaN to integer"
